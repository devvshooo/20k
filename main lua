-- Create Secret Tab
local SecretTab = Window:CreateTab("Secret Tab", 4483362458) 

-- Trap City Auto Community Service (Fast Teleport) Button
local AutoCommService = SecretTab:CreateButton({
    Name = "Trap City Auto Community Service",
    Callback = function()
        local player = game.Players.LocalPlayer
        local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
        local serviceRunning = true -- set to false to stop

        local function tp(pos)
            if hrp then
                hrp.CFrame = CFrame.new(pos + Vector3.new(0, 5, 0))
            end
        end

        local function cleanPrompt(prompt)
            if prompt and prompt:IsA("ProximityPrompt") then
                fireproximityprompt(prompt, 1)
            end
        end

        task.spawn(function()
            while serviceRunning do
                for _, obj in pairs(workspace:GetDescendants()) do
                    if obj:IsA("ProximityPrompt") and (obj.Parent.Name:lower():find("clean") or obj.Parent.Name:lower():find("trash")) then
                        tp(obj.Parent.Position)
                        task.wait(0.15) -- short wait for teleport settle
                        cleanPrompt(obj)
                        task.wait(0.5) -- short wait for cleaning animation
                    end
                end
                task.wait(0.1)
            end
        end)
    end
})
