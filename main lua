local Button = MainTab:CreateButton({
    Name = "Big Head (ALL Players)",
    Callback = function()
        local Players = game:GetService("Players")

        local function BigHead(character)
            for _, part in pairs(character:GetChildren()) do
                if part:IsA("BasePart") and part.Name == "Head" then
                    part.Size = part.Size * 3 -- change 3 to adjust size
                elseif part:IsA("Accessory") and part:FindFirstChild("Handle") then
                    part.Handle.Size = part.Handle.Size * 3
                end
            end
        end

        -- Apply to all current players
        for _, player in pairs(Players:GetPlayers()) do
            if player.Character then
                BigHead(player.Character)
            end
        end

        -- Apply to players when they respawn
        Players.PlayerAdded:Connect(function(player)
            player.CharacterAdded:Connect(function(char)
                wait(1)
                BigHead(char)
            end)
        end)

        -- Apply to already connected players respawning
        for _, player in pairs(Players:GetPlayers()) do
            player.CharacterAdded:Connect(function(char)
                wait(1)
                BigHead(char)
            end)
        end
    end
})

